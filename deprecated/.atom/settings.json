{
	"*": {
		"core": {
			"disabledPackages": [
				"language-gfm",
				"sync-settings-git-location"
			],
			"telemetryConsent": "limited"
		},
		"editor": {
			"fontSize": 17,
			"showIndentGuide": true,
			"showInvisibles": true
		},
		"exception-reporting": {
			"userId": "REDACTED"
		},
		"git-blame": {
			"columnWidth": 392
		},
		"minimap": {
			"plugins": {
				"highlight-selected": true,
				"highlight-selectedDecorationsZIndex": 0
			}
		},
		"sync-settings": {
			"hiddenSettings": {},
			"useOtherLocation": true
		},
		"sync-settings-folder-location": {
			"folderPath": "~/dotfiles/.atom"
		},
		"tree-view": {
			"hideVcsIgnoredFiles": true
		}
	}
}